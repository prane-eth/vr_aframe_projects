nction dollFaceCamera() {         doll.setAttribute(             "animation",             "property: rotation; to: 0 0 0; dur: " +                 rotationDuration +                 "; easing: linear;"         );         await sleep(rotationDuration);     }      /* rotate doll to face wall */     async function dollFaceWall() {         doll.setAttribute(             "animation",             "property: rotation; to: 0 180 0; dur: " +                 rotationDuration +                 "; easing: linear;"         );         await sleep(rotationDuration);     }      function sleep(ms) {         return new Promise((resolve) =&gt; setTimeout(resolve, ms));     }      /* wait random time between 2 to 8 seconds */     async function waitRandomTime() {         var ms = Math.floor(Math.random() * 4000) + 2000;         console.log("waiting for " + ms + " ms");         await sleep(ms);     }      var keyPressed = false;     /* change value when w is pressed */     document.addEventListener("keydown", function (event) {         if (event.keyCode == 87 &amp;&amp; !keyPressed) {             keyPressed = true;         }     });     document.addEventListener("keyup", function (event) {         if (event.keyCode == 87 &amp;&amp; keyPressed) {             keyPressed = false;         }     });      function isGameFinished() {         var posZ = camera.object3D.position.z;         var finishLineZ = finishLine.object3D.position.z;         if (posZ &lt; finishLineZ) {             return true;         } else {             return false;         }     }      async function startRunning() {         await waitRandomTime();         sing.play();         await dollFaceWall();         stopRunning();     }      async function stopRunning() {         await waitRandomTime();         sing.pause();         await dollFaceCamera();          if (isGameFinished()) {             console.log("You won!");             /* animate ground color between red and current color */             var currentColor = ground.getAttribute("color");             var newColor = "green";             ground.setAttribute(                 "animation",                 "property: components.material.material.color; type: color; to: " +                     newColor +                     "; dur: 1000; easing: linear; dir: alternate; loop: true;"             );             winSong.play();             return;         }         if (keyPressed) {             console.log("You lost!");             /* animate ground color between red and current color */             var currentColor = ground.getAttribute("color");             var newColor = "red";             /* make camera not movable with wasd controls */             camera.setAttribute("look-controls", "enabled: false");             camera.setAttribute("wasd-controls", "enabled: false");             ground.setAttribute(                 "animation",                 "property: components.material.material.color; type: color; to: " +                     newColor +                     "; dur: 1000; easing: linear; dir: alternate; loop: true;"             );             kill.play();             return;         }         startRunning();     }      window.onload = startRunning; &lt;/script&gt;</field></block></next></block></statement></block></next></block></statement></block></xml>